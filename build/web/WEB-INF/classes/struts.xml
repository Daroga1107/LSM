<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.devMode" value="true" />
    <package name="Actions" extends="struts-default" >
        <action name="Login" class="Actions.Login" >
            <result name="input">index.jsp</result>
            <result name="administrador">admin.jsp </result>
            <result name="estudiante" >student.jsp</result>
            <result name="profesor">teacher.jsp</result>
        </action>
        <action name="Register" class="Actions.Register" method="execute">
            <result name="success" >index.jsp</result>
            <result name="input" >register.jsp</result>
            
        </action>
    </package>
</struts>
